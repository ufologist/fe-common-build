{
  "name": "fe-common-build-example",
  "version": "0.1.0",
  "description": "前端工程化之通用构建方案",
  "main": "gulpfile.js",
  "scripts": {
    "dev": "npm-run-all --parallel \"build:css -- --watch --env=dev\" \"build:js -- --watch --env=dev\" \"build:res -- --watch --env=dev\"",
    "build": "npm-run-all build:clean build:css build:js build:res",
    "build:css": "gulp css",
    "build:js": "gulp js",
    "build:res": "gulp res",
    "build:clean": "gulp clean",
    "deploy": "gulp deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "fe-common-build": "0.x",
    "npm-run-all": "^4.0.2"
  },
  "author": "Sun",
  "license": "MIT"
}
